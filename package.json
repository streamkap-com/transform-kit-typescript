{
  "name": "transform-kit-typescript",
  "version": "1.0.0",
  "description": "TypeScript development environment (with esbuild) for building robust, testable Streamkap transforms with npm ecosystem support.",
  "main": "build-multiple.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/streamkap-com/transform-kit-typescript.git"
  },
  "homepage": "https://docs.streamkap.com/docs/transforms/typescript-development-kit",
  "bugs": {
    "url": "https://github.com/streamkap-com/transform-kit-typescript/issues"
  },
  "scripts": {
    "build": "node build-multiple.js --all",
    "build:map-filter": "node build-multiple.js --map-filter",
    "build:fan-out": "node build-multiple.js --fan-out",
    "build:enrich-async": "node build-multiple.js --enrich-async",
    "build:un-nesting": "node build-multiple.js --un-nesting",
    "bundle:streamkap": "node build-multiple.js",
    "test": "node test-selective.js",
    "test:map-filter": "npm run build:map-filter && node test-selective.js --map-filter",
    "test:fan-out": "npm run build:fan-out && node test-selective.js --fan-out", 
    "test:enrich-async": "npm run build:enrich-async && node test-selective.js --enrich-async",
    "test:un-nesting": "npm run build:un-nesting && node test-selective.js --un-nesting",
    "test:watch": "jest --watch",
    "test:coverage": "npm run build && jest --coverage"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/moment": "^2.11.29",
    "@types/uuid": "^10.0.0",
    "esbuild": "^0.8.27",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^4.0.0"
  },
  "keywords": [
    "streamkap",
    "transforms",
    "typescript",
    "bundling",
    "esbuild",
    "data-transformation",
    "development-kit"
  ],
  "author": "Streamkap",
  "license": "MIT"
}
